<%@ jet 
package="org.skyway.integration.java.spring.jet.operation" 
class="OperationNestedMethodInputsTemplate"
%>
<%@taglib prefix="java" id="org.eclipse.jet.javaTags" %>
<%@taglib prefix="ws"   id="org.eclipse.jet.workspaceTags"%>
<%@taglib prefix="c"    id="org.eclipse.jet.controlTags"%>
<%@taglib prefix="f"    id="org.eclipse.jet.formatTags"%>
<%@taglib prefix="sw"   id="org.skyway.integration.java.skywayCodeGenTags"%>
<c:iterate select="$currentOperation/inputs" var="input" delimiter=","><sw:declarationAlias select="$input" alias="inputAliasName"/><sw:declareMethodVariable select="$input"><java:import><sw:javaType select="$input" package="true" alias="{$inputAliasName}" /></java:import></sw:declareMethodVariable></c:iterate><c:if test="$currentOperation/@inputsSize > 0 and $currentOperation/@variablesSize > 0">,</c:if><c:iterate select="$currentOperation/variables" var="variable" delimiter=","><sw:declareMethodVariable select="$variable"><java:import><sw:javaType package="true" select="$variable" alias="{$aliasName}"/></java:import></sw:declareMethodVariable></c:iterate><c:if test="($currentOperation/@inputsSize > 0 or $currentOperation/@variablesSize > 0) and $currentOperation/@constantsSize > 0">,</c:if><c:iterate select="$currentOperation/constants" var="constant" delimiter=","><sw:declareMethodVariable select="$constant"><java:import><sw:javaType package="true" select="$constant" alias="{$aliasName}"/></java:import></sw:declareMethodVariable></c:iterate>